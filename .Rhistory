library(tidymodels)
library(tidyverse)
set.seed(12)
nsim <- 8
points <- tibble(
a = runif(nsim, 0, 5),
b = a * 2 / 3 + rnorm(nsim, 0, 1)
)
points <- bind_cols(
points,
points %>%
recipe(~a + b) %>%
step_center(all_numeric()) %>%
prep() %>%
bake(new_data = NULL) %>%
rename(
a_centered = a,
b_centered = b
),
points %>%
recipe(~a + b) %>%
step_normalize(all_numeric()) %>%
prep() %>%
bake(new_data = NULL) %>%
rename(
a_scaled = a,
b_scaled = b
),
points %>%
recipe(~a + b) %>%
step_normalize(all_numeric()) %>%
step_pca(all_numeric()) %>%
prep() %>%
bake(new_data = NULL)
)
points %>%
ggplot(aes(x = a, y = b)) +
geom_point()
